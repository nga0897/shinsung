        }

        .h_12 {
            height: 12vh;
        }

        .h_10 {
            height: 10vh;
        }

        .pd_2vh {
            padding: 2vh;
        }

        .h_50 {
            height: 50vh;
            font-size: 16vw;
        }

        .bg_reg {
            background: #d81624;
        }

        .bg_blue {
            background: #00c6bb;
        }

        .bg {
            background-image: url("../Images/bg.jpg");
            background-repeat: no-repeat;
        }

        .border {
            border: 3px solid white;
        }

        iframe {
            overflow: hidden;
        }
