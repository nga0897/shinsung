    <style>
        .slick-slide img {
            max-height: 65vh;
        }

        .image {
            height: 100%;
        }

        body {
            font-size: 2vw;
            font-family: Oswald;
        }

        .hidden {
            display: none;
        }

        .active {
            display: block;
        }

        .pt-page {
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            visibility: hidden;
            overflow: hidden;
            -webkit-backface-visibility: hidden;
            -moz-backface-visibility: hidden;
            backface-visibility: hidden;
            -webkit-transform: translate3d(0, 0, 0);
            -moz-transform: translate3d(0, 0, 0);
            transform: translate3d(0, 0, 0);
            -webkit-transform-style: preserve-3d;
            -moz-transform-style: preserve-3d;
            transform-style: preserve-3d;
        }

        .pt-page-current, .no-js .pt-page {
            visibility: visible;
            z-index: 1;
        }

        .all_cg {
            display: flex;
            justify-content: center;
            align-items: center;
        }

        form#reset_page {
            text-align: center;
        }

        h2 {
            text-transform: uppercase;
            font-size: 2.3vw;
            padding-top: 2vh;
        }

        h3 {
            font-size: 1.2vw;
        }

        h4.xanh {
            font-size: 12.5vw;
            text-align: center;
            vertical-align: middle;
            line-height: 38vh;
        }

        h4.vang {
            font-size: 12.5vw;
            text-align: center;
            vertical-align: middle;
            line-height: 20vh;
        }

        h2 {
            margin-top: 0px;
        }

        .h-45 {
            height: 20vh;
        }

        form#reset_page,
        form#reset_page3,
        .full_height {
            height: 100vh;
        }

        /*.row.bg-dark {
            height: 8vh;
        }*/

        .embed-responsive-5by4 {
            position: absolute;
            width: 100%;
            height: 100%;
        }

        .bg-dark {
            background: #170000;
        }

        .text-white {
            color: white;
        }

        .bg-light {
            height: 20vh;
            background: #534b4b;
        }

        .text-left {
            text-align: left;
        }

        .text-right {
            text-align: right;
        }

        .pd_1vh {
            padding: 1vh;
            font-size: 3vw;
        }

        .mg_4vh {
            MARGIN-TOP: 2VH;
        }

        .h_72 {
