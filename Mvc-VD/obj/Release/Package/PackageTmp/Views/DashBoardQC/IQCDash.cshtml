@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_LayoutSI.cshtml";
}

<link href="~/Content/important.css" rel="stylesheet" />

@section Tilte {
    <div class="col-xs-12" id="current_menu" data-level_1="QMS" data-level_2="Dash Board" data-level_3="IQC Dash"></div>
}
<div class="container-fluid">
    <div class="box-body my-2 bg-white">
        <div class="row p-0 m-0">

            <div class="col-md-1 p-1  text-right">
                <label><b>Chart Type </b></label>
            </div>
            <div class="col-md-1 p-1">
                <select name="barchart" id="barchart" class="form-control " >
                    <option value="1">Bar Chart</option>
                    <option value="2">Pie Chart</option>
                </select>
            </div>

            @* nam test *@
            <div class="col-md-1 p-1  text-right">
                <label><b>MWR No </b></label>
            </div>
            <div class="col-md-2 p-1">
                <div class="input-group mt_plus">
                    <div class="input-group mt_plus">
                        <input type="text" name="mwro_no" id="s_mwro_no" class="input-text form-control">
                        <span class="input-group-addon psbom"><a href="#" class="poupdialogl"><i class="fa fa-search text-navy"></i></a></span>
                    </div>
                </div>
            </div>
            @* end *@

            <div class="col-md-1 p-1  text-right">
                <label><b>MT NO </b></label>
            </div>
            <div class="col-md-2 p-1">
                <div class="input-group mt_plus">
                    <div class="input-group mt_plus">
                        <input type="text" name="s_mt_no" id="s_mt_no" class="input-text form-control">
                        <span class="input-group-addon psbom"><a href="#" class="poupdialog"><i class="fa fa-search text-navy"></i></a></span>
                    </div>
                </div>
            </div>

            <div class="col-md-1 p-1  text-right">
                <label><b>Receive Date</b></label>
            </div>
            <div class="col-md-1 p-1">
                @*                <input type="text" name="rec_input_dt" id="start" style="text-align: center" class="form-control hasDatepicker">*@
                <input type="text" name="start" id="start" class="form-control form-control-text">
            </div>
            <div class="col-md-0 p-1  text-right">
                <label><b>~</b></label>
            </div>
            <div class="col-md-1 p-1">
                @*                <input type="text" name="rec_input_dt" id="end" style="text-align: center" class="form-control hasDatepicker">*@
                <input type="text" name="end" id="end" class="form-control form-control-text">
            </div>
            <div class="col-md-1 p-2">
                <button id="searchBtn" class="btn btn-sm btn-primary button-srh"><i class="fa fa-search">&nbsp;Search</i></button>
            </div>

        </div>
    </div>

</div>
@*--Popup--*@

