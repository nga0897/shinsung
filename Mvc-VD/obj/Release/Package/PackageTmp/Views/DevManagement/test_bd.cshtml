<div id="tree"></div>


<script>


    $.get("/DevManagement/Gettable_data?line_no=" + "LN00466").done(function (response) {
        console.log(response.nodes);


        var chart = new OrgChart(document.getElementById("tree"), {
            //orientation: OrgChart.orientation.left,
            enableDragDrop: true,
            nodeMenu: {
                edit: { text: "Edit" },
                add: { text: "Add" },
                remove: { text: "Remove" }
            },
                
            nodeBinding: {
                field_0: "name"
            },
            nodes: response.nodes,

        });
    });
</script>
