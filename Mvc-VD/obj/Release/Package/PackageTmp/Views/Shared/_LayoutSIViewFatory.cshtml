    <style>


        #loading {
            width: 100%;
            height: 100%;
            top: 0px;
            left: 0px;
            position: fixed;
            display: none;
            opacity: 0.7;
            background-color: #fff;
            z-index: 1000;
            text-align: center;
        }

        #loading-image {
            position: absolute;
            top: 45%;
            left: 45%;
            width: 100px;
            height: 100px;
            z-index: 1001;
        }

        .ui-front {
            z-index: 1000;
        }

        body .bg-dialog-loading {
            width: 100%;
            height: 100%;
            position: absolute;
            top: 0;
            left: 0;
            display: none;
            background: #333333;
            background: rgba(51, 51, 51, 0.5);
            z-index: 999;
        }

        body.dialog-loading .bg-dialog-loading {
            display: block;
        }

        body .bg-dialog-loading .image-dialog-loading {
            position: absolute;
            top: 0;
            left: 0;
            right: 0;
            bottom: 0;
            margin: auto;
        }

        i.fa.fa-language {
            font-size: 43px;
            color: yellow;
            padding: 10px;
        }
    </style>
