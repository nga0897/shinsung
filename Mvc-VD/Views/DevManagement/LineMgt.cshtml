                                <script>
                                    function formatDate() {
                                        var d = new Date(),
                                            month = '' + (d.getMonth() + 1),
                                            day = '' + d.getDate(),
                                            year = d.getFullYear(),
                                            Hours = d.getHours(),
                                            Minutes = d.getMinutes();

                                        if (month.length < 2) { month = '0' + month; }
                                        if (day.length < 2) { day = '0' + day; }
                                        if (Hours.length < 2) { Hours = '0' + Hours; }
                                        if (Minutes.length < 2) { Minutes = '0' + Minutes; }

                                        time = Hours + ":" + Minutes;
                                        var full = year + "-" + month + "-" + day + " " + time;
                                        $("#datetime").html(full);
                                        $("#datetime1").html(full);
                                    }
                                    formatDate()
                                    setInterval(formatDate, 500);
                                </script>
                            </div>
                        </div>
                    </div>
