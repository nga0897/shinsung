                            </div>
                            <div class="col-lg-1 py-1">
                                <select name="use_yn" id="dc_use_yn" class="form-control select2">
                                    <option value="Y" selected="selected">Use</option>
                                    <option value="N">UnUse</option>
                                </select>
                            </div>
                            <div class="col-lg-2 py-2 d-flex">
                                <button type="button" id="dc_save_but" class="btn btn-success button-srh2"><i class="fa fa-save">&nbsp;Create</i></button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="tab-content hidden" id="tab_dc2">
                <form id="form4">
                    <div class="box-body box-body-bottom-input">
                        <div class="row p-2">
                            <div class="col-lg-1 py-1">
                                <input type="text" name="cdid" id="dm_cdid_cd" class="form-control form-control-text" placeholder="No" readonly="readonly">
                            </div>
                            <div class="col-lg-1 py-1">
                                <input type="text" name="mt_cd" id="dm_mt_cd" class="form-control form-control-text" placeholder="Main Code" readonly="readonly">
                            </div>
                            <div class="col-lg-1 py-1">
                                <input type="text" name="dt_cd" id="dm_dt_cd" class="form-control form-control-text" placeholder="Detail Code" readonly="readonly">
                            </div>
                            <div class="col-lg-2 py-1">
                                <input type="text" name="dt_nm" id="dm_dt_nm" class="form-control form-control-text" placeholder="Detail Code Name">
                            </div>
                            <div class="col-lg-2 py-1">
                                <input type="text" name="dt_exp" id="dm_dt_exp" class="form-control form-control-text" placeholder="Detail Code Explain">
                            </div>
                            <div class="col-lg-2 py-1">
                                <input type="text" name="dt_order" id="dm_dt_order" class="form-control form-control-text" placeholder="Detail Order">
                            </div>
                            <div class="col-lg-1 py-1">
                                <select name="use_yn" id="dm_use_yn" class="form-control select2">
                                    <option value="Y" selected="selected">Use</option>
                                    <option value="N">UnUse</option>
                                </select>
                            </div>
                            <div class="col-lg-2 py-2 d-flex">
                                <button type="button" id="dm_save_but" class="btn btn-success button-srh2 mr-5"><i class="fa fa-save">&nbsp;Modify</i></button>
                                <button type="button" id="ddel_save_but" class="btn btn-danger button-srh2"><i class="fa fa-scissors">&nbsp;Delete</i></button>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

    <!--Popup -->
    <div id="dialogDangerous_cm" title="Are you make sure DELETE main common?">
        <div class="container-fluid">
            <div class="box-body bg-white text-right">
                <div class="row p-0 m-0">
                    <div class="col-md-12 p-2">
                        <button type="button" class="btn btn-sm btn-success button-srh mr-3" id="closestyle">No</button>
                        <button type="button" class="btn btn-sm btn-danger button-srh" id="deletestyle">Yes</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ./ Popup -->
    <!--Popup -->
    <div id="dialogDangerous_dt" title="Are you make sure DELETE common detail?">
        <div class="container-fluid">
            <div class="box-body bg-white text-right">
                <div class="row p-0 m-0">
                    <div class="col-md-12 p-2">
                        <button type="button" class="btn btn-sm btn-success button-srh mr-3" id="closestyle_dt">No</button>
                        <button type="button" class="btn btn-sm btn-danger button-srh" id="deletestyle_dt">Yes</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ./ Popup -->

    @section scripts{
        <script src="~/Scripts/DevManagement/DevelopCommon/_DevelopCommon.js"></script>
        <script src="~/Scripts/DevManagement/DevelopCommon/_DevelopCommon_Poup.js"></script>
    }
