@{
    ViewBag.Title = "QMS";
    Layout = "~/Views/Shared/_LayoutSI.cshtml";
}
@section Tilte {
    @*<div class="col-xs-12" id="current_menu" data-level_1="QMS" data-level_2="QC Information" data-level_3="PQC List"></div>*@
    <div class="col-xs-12" id="current_menu" data-level_1="@Html.Raw(ViewData["QMS"])" data-level_2="@Html.Raw(ViewData["QC Information"])" data-level_3="@Html.Raw(ViewData["PQC List"])"></div>
}



<div class="container-fluid">
    <div class="row text-right">
        <div class="col-md-12 px-3">
            <button type="submit" id="excelBtn" class="btn btn-sm btn-warning button-top"><i class="fa fa-file-excel-o">&nbsp;Excel</i></button>
            <button type="submit" id="htmlBtn" class="btn btn-sm btn-warning button-top" hidden="hidden"><i class="fa fa-print">&nbsp;HTML</i></button>
        </div>
    </div>
</div>

<div class="container-fluid">
    <div class="box-body my-2 bg-white">

        <div class="row p-0 m-0">


            <div class="col-md-1 p-1  text-right">
                <label><b>FQ No </b></label>
            </div>
            <div class="col-md-1 p-1">
                <input type="text" name="fq_no" id="fq_no" class="input-text form-control">
            </div>
            @*<div class="col-md-1 p-1  text-right">
                    <label><b>Product </b></label>
                </div>
                <div class="col-md-1 p-1">
                    <div class="input-group mt_plus">
                        <input type="text" name="style_no" id="style_no" class="input-text form-control">
                        <span class="input-group-addon psbom"><a href="#" id="poupdialog2" class="poupdialog2"><i class="fa fa-search text-navy"></i></a></span>
                    </div>
                </div>*@
            <div class="col-md-1 p-1  text-right">
                <label><b>Material Lot Code </b></label>
            </div>
            <div class="col-md-1 p-1">
                <input type="text" name="ml_no" id="ml_no" class="input-text form-control">
            </div>
            <div class="col-md-1 px-1 py-2  text-right">
                <label><b>Work Date</b></label>
            </div>
            <div class="col-md-1 p-1">
                <input type="text" name="start" id="start" class="form-control">
            </div>
            <div class="col-md-0 px-1 py-2 text-right">
                <label><b>~</b></label>
            </div>
            <div class="col-md-1 p-1">
                <input type="text" name="end" id="end" class="form-control">
            </div>
            <div class="col-md-1 p-2">
                <button id="searchBtn" class="btn btn-sm btn-primary button-srh"><i class="fa fa-search">&nbsp;Search</i></button>
            </div>

        </div>

    </div>

</div>


<div class="container-fluid">
    <div class="row p-0 m-0">
        <div class="col-xs-12 pl-0">
            <div class="box-body my-2 bg-white" id="two">
